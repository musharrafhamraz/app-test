{
  "expo": {
    "name": "Musify",
    "slug": "Disease-Detection-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icons/app.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Icons/app.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.musharrafhamraz.emotionMusic"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "958b3c97-25ea-4eeb-a6bc-1c282012c33a"
      }
    }
  }
}
